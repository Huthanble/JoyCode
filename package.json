{
  "name": "joycode",
  "displayName": "JoyCode",
  "description": "This is a code intelligent completion plugin that helps you enjoy coding.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:python",
    "onLanguage:java",
    "onLanguage:c",
    "onLanguage:cpp"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "joycode.tab",
        "title": "捕获Tab Command"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.98.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.21.0",
    "vscode": "^1.1.37",
    "glob": "^7.2.0",
    "mocha": "^9.2.0"
  }
}
